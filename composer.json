{
    "name": "src-run/augustus-instantiator-library",
    "license": "MIT",
    "type": "library",
    "homepage": "https://src.run/augustus-instantiator-library",
    "description": "Utility to instantiate objects providing both a stateless and a stateful implementation.",
    "support": {
        "email": "rmf@src.run",
        "source": "https://src.run/augustus-instantiator-library/git",
        "issues": "https://src.run/augustus-instantiator-library/issues"
    },
    "keywords": [
        "augustus-instantiator-library", "augustus", "instantiator", "library", "rmf", "src-run"
    ],
    "authors": [
        {
            "name": "Rob Frawley 2nd",
            "email": "rmf@src.run",
            "homepage": "https://src.run/rmf",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": "^7.1",
        "src-run/augustus-utility-library": "^0.9.2"
    },
    "require-dev": {
        "codacy/coverage": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-coveralls/php-coveralls": "^2.0",
        "symfony/phpunit-bridge": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "SR\\Instantiate\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SR\\Instantiate\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}
